{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>Eviive/renovate-config:base",
    "github>Eviive/renovate-config//kubernetes/base"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchDepTypes": [
        "HelmChart"
      ],
      "matchPackageNames": [
        "registry-1.docker.io/bitnamicharts/postgresql"
      ],
      "pinDigests": false
    }
  ],
  "ignoreDeps": [
    "alexfozor/flaresolverr"
  ]
}
